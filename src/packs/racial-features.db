hese creatures awaken normally if wounded.","source":"","subtab":"misc","valueAffected":"per"}],"range":{"additional":"","per":"","units":"none","value":null},"recharge":{"charged":false,"value":null},"requirements":"Varculak","rollNotes":"","save":{"type":"","dc":null,"descriptor":""},"source":"AA3 pg. 126","target":{"type":"","value":""},"uses":{"max":null,"per":"","value":null},"details":{"category":"speciesFeature"}}}
