harrying":false,"holyWater":false,"ignite":false,"indirect":false,"integrated":false,"lockdown":false,"mine":false,"mire":false,"modal":false,"necrotic":false,"polarize":false,"polymorphic":false,"professional":false,"punchGun":false,"recall":false,"relic":false,"reposition":false,"shape":false,"shells":false,"shield":false,"sunder":false,"swarm":false,"tail":false,"teleportive":false,"thought":false,"throttle":false,"underwater":false,"variantBoost":false,"wideLine":false},"proficient":true,"fusions":[],"abilityMods":{"parts":[]}},"flags":{},"img":"icons/svg/mystery-man.svg"}
{"_id":"wKVMw34GyIVXXs0w","name":"Serum of Healing, Mk 1","permission":{"default":0,"umHu8DFnCrMQVZ37":3},"type":"consumable","data":{"description":{"value":"<p><span id=\"ctl00_MainContent_DataListMagicItems_ctl11_LabelName\">This vial of medical serum quickly sets bones and repairs other physical damage. It restores Hit Points when administered to a living creature.&nbsp;&nbsp;</span></p>\n<p><span id=\"ctl00_MainContent_DataListMagicItems_ctl11_LabelName\">Serums are vials of magic liquid that you can imbibe as a standard action or carefully trickle down the throat of a helpless or unconscious creature as a full action. Serums are normally 1 ounce of liquid (though it is possible to make serums with larger volumes, these are no more effective than standard serums), and lose potency if they are mixed with any other material. A serum can be used only once. Regardless of a serum’s actual item level, all serum vials have their hardness and Hit Points calculated as if they were 1st-level items.&nbsp;</span></p>","chat":"","unidentified":""},"source":"Core Rulebook","type":"","quantity":1,"bulk":"L","price":50,"level":1,"attuned":false,"equipped":false,"identified":true,"activation":{"type":"action","cost":1,"condition":""},"duration":{"value":"","units":""},"target":{"value":"","type":""},"area":{"value":null,"units":"none","shape":"","effect":""},"range":{"value":null,"units":"none","additional":"","per":""},"uses":{"value":1,"max":1,"per":"charges","autoUse":true,"autoDestroy":true},"ability":"","actionType":"heal","attackBonus":0,"chatFlavor":"","critical":{"parts":[],"effect":""},"damage":{"parts":[["1d8","healing"]]},"formula":"","save":{"type":"","dc":null,"descriptor":"negate"},"descriptors":[],"consumableType":"serum"},"flags":{},"img":"systems/sfrpg/icons/equipment/magic%20items/serum-of-healing.jpg"}
{"_id":"wNdwVbEu21geKDBD","name":"Pike, elite","permission":{"default":0},"type":"weapon","data":{"description":{"value":"<p>A pike is composed of a sharpened-aluminum, stainless-steel, or carbon-steel spike atop a light metal or polycarbonate staff.</p>","chat":"","unidentified":""},"source":"Core Rulebook, P. 172","type":"","quantity":1,"bulk":"2","price":95700,"level":15,"attuned":false,"equipped":false,"identified":true,"activation":{"type":"none","cost":0,"condition":""},"duration":{"value":"","units":""},"target":{"value":"","type":""},"area":{"value":null,"units":null,"shape":"","effect":""},"range":{"value":null,"units":null,"additional":"","per":""},"uses":{"value":0,"max":0,"per":""},"ability":"str","actionType":"mwak","attackBonus":0,"chatFlavor":"","critical":{"parts":[],"effect":""},"damage":{"parts":[["7d8 + @mod","piercing"]]},"formula":"","save":{"type":"","dc":null,"descriptor":"negate"},"descriptors":[],"capacity":{"value":0,"max":0},"usage":{"value":0,"per":""},"weaponType":"advancedM","special":"","properties":{"one":false,"two":true,"amm":false,"analog":true,"archaic":false,"automatic":false,"blast":false,"block":false,"boost":false,"bright":false,"disarm":false,"entangle":false,"explode":false,"injection":false,"line":false,"nonlethal":false,"operative":false,"penetrating":false,"powered":false,"qreload":false,"reach":true,"sniper":false,"stun":false,"thrown":false,"trip":false,"unwieldy":false},"proficient":true,"fusions":[]},"sort":1500001,"flags":{},"img":"icons/svg/mystery-man.svg"}
{"_id":"wVqkryVMIaeHne6U","name":"Battery, High-Capacity","permission":{"default":0,"umHu8DFnCrMQVZ37":3},"type":"goods","data":{"description":{"value":"<p><span id=\"ctl00_MainContent_DataListTalentsAll_ctl00_LabelName\">Batteries charge powered weapons, but they can also be used to power an array of items, including powered armor and technological items. Batteries have a standardized size and weight, and items that take batteries all have a slot into which they fit, regardless of the item&rsquo;s actual size. Weapons that use batteries list the highest-capacity battery they are capable of using as well as how many charges from the battery that each shot consumes.&nbsp;</span></p>\n<p><strong>Capacity:</strong> 40</p>","chat":"","unidentified":""},"source":"Core Rulebook","type":"","quantity":1,"bulk":"-","price":330,"level":4,"attuned":false,"equipped":false,"identified":true,"damage":{"parts":[]}},"flags":{},"img":"systems/sfrpg/icons/equipment/weapons/battery-high-capacity.jpg"}
{"name":"Brigh's Bolt","permission":{"default":0,"umHu8DFnCrMQVZ37":3},"type":"goods","data":{"description":{"value":"<p><span id=\"ctl00_MainContent_DataListMagicItems_ctl00_LabelName\">This simple, shiny metal bolt is the size of a thumbnail and has an image of a tiny gear on its head. As a full action, you can affix a <em>Brigh’s bolt</em> to a broken suit of armor, tool, vehicle, or weapon to negate the penalties of the broken condition for as long as the bolt is affixed. The bolt does not repair any damage sustained by the object and the object is still visibly damaged, so its resale value is reduced as normal for a broken item. If the item is repaired so as to no longer have the broken condition, the Brigh’s bolt falls off; otherwise, it can be removed as a full action. If an item to which a <em>Brigh’s bolt</em> is attached is destroyed, the bolt is also destroyed.&nbsp;</span></p>","chat":"","unidentified":""},"source":"Pact Wprlds","type":"","quantity":1,"bulk":"-","price":1300,"level":3,"attuned":false,"equipped":false,"identified":true,"damage":{"parts":[]}},"flags":{},"img":"systems/sfrpg/icons/equipment/magic%20items/brighs-bolt.jpg","_id":"wYDXX5KDz8Rfe65n"}
{"_id":"wfEhTbbOLZbHzX6B","name":"Energetic","permission":{"default":0,"umHu8DFnCrMQVZ37":3},"type":"fusion","data":{"description":{"value":"<p><span id=\"ctl00_MainContent_DataListWeaponFusions_ctl00_LabelName\">A projectile weapon with the <em>energetic</em> fusion relies on energy rather than physical ammunition to generate kinetic attacks. Instead of being loaded with darts, rounds, or scattergun shells, the weapon is loaded with a battery. The type of battery the projectile weapon uses depends on its original capacity as follows: 1–20 use batteries, 21–40 use high-capacity batteries, 41–80 use super-capacity batteries, and 81 or more use ultracapacity batteries. The weapon’s usage does not change, but it takes charges from the installed battery, rather than expending physical ammunition. The fusion converts the energy into physical bullets which target KAC and deal damage normally for the weapon. Only weapons in the projectile category that would normally use darts, rounds, or scattergun shells can benefit from the <em>energetic</em> fusion.&nbsp;</span></p>","chat":"","unidentified":""},"source":"Armory","type":"","level":5,"damage":{"parts":[]}},"flags":{},"img":"systems/sfrpg/icons/equipment/weapons/weapon-fusions-5.jpg"}
{"_id":"wrRXJNlnDICvqKY6","name":"Serum of Hibernation, Potent","permission":{"default":0,"umHu8DFnCrMQVZ37":3},"type":"goods","data":{"description":{"value":"<p><span id=\"ctl00_MainContent_DataListMagicItems_ctl00_LabelName\">Scientists studying the hibernation-adapted flora native to Gluun isolated enzymes and compounds that aided in the species’ long sleep. Further refinement led to this serum, which is typically available only through medical professionals or military groups, accompanied by warnings and waivers. Most who use this serum do so to make long-distance spaceflight more tolerable or efficient, especially when moving large groups with limited space and resources.<br></span></p>\n<p><span id=\"ctl00_MainContent_DataListMagicItems_ctl00_LabelName\">Upon drinking a <em>serum of hibernation</em>, you can allow yourself to fall into a deathlike trance for a time that depends on the potency of the serum. While in this hibernation, you need not eat, drink, or breathe. You awaken if you take damage or if anyone uses a medkit or more advanced medical technology to rouse you and succeeds at a DC 15 Medicine check. If roused before the duration ends, unless the Medicine check to rouse you succeeds by 10 or more, you suffer 1 temporary negative level and are exhausted. Anyone interacting with your hibernating body can tell you are alive with a successful DC 25 Perception check, but someone performing a medical exam can determine your condition with a successful DC 15 Medicine check.&nbsp;</span></p>\n<ul>\n<li><strong>Diluted (Level 1):</strong> 2d4 days.</li>\n<li><strong>Potent (Level 5):</strong> 2d4 x 3 days.</li>\n</ul>","chat":"","unidentified":""},"source":"Huskworld","type":"","quantity":1,"bulk":"L","price":400,"level":5,"attuned":false,"equipped":false,"identified":true,"damage":{"parts":[]}},"flags":{},"img":"systems/sfrpg/icons/equipment/magic%20items/serum-of-hibernation.jpg"}
{"_id":"x2yj6YMNGefgBzct","name":"Arc pistol, static","permission":{"default":0},"type":"weapon","data":{"description":{"value":"<p>Arc pistols fire deadly, stunning blasts of electricity at ranged targets. They have a two-pronged emitter that directs the electrical blast and a bulky chamber that holds the capacitor.</p>","chat":"","unidentified":""},"source":"Core Rulebook, P. 173","type":"","quantity":1,"bulk":"L","price":750,"level":2,"attuned":false,"equipped":false,"identified":true,"activation":{"type":"none","cost":0,"condition":""},"duration":{"value":"","units":""},"target":{"value":"","type":""},"area":{"value":null,"units":"none","shape":"","effect":""},"range":{"value":50,"units":"ft","additional":"","per":""},"uses":{"value":0,"max":0,"per":""},"ability":"dex","actionType":"rwak","attackBonus":0,"chatFlavor":"","critical":{"parts":[],"effect":""},"damage":{"parts":[["1d6","electricity"]]},"formula":"","save":{"type":"","dc":null,"descriptor":"negate"},"descriptors":[],"capacity":{"value":20,"max":20},"usage":{"value":2,"per":"round"},"weaponType":"smallA","special":"","properties":{"one":true,"two":false,"amm":false,"analog":false,"archaic":false,"automatic":false,"blast":false,"block":false,"boost":false,"bright":false,"disarm":false,"entangle":false,"explode":false,"injection":false,"line":false,"nonlethal":false,"operative":false,"penetrating":false,"powered":false,"qreload":false,"reach":false,"sniper":false,"stun":true,"thrown":false,"trip":false,"unwieldy":false},"proficient":true,"fusions":[]},"sort":200001,"flags":{},"img":"systems/sfrpg/icons/equipment/weapons/arc-pistol.jpg"}
{"_id":"x3nWbWSU1nEMhuSE","name":"Artillery Laser, Aphelion","permission":{"default":0,"umHu8DFnCrMQVZ37":3},"type":"weapon","data":{"description":{"value":"<p><span id=\"ctl00_MainContent_DataListTalentsAll_ctl00_LabelName\">Artillery lasers are heavy weapons designed to be effective against hardened targets such as vehicles and enemy bunkers.</span></p>\n<h3 class=\"framing\"><strong>Laser Weapons</strong></h3>\n<hr>\n<p>Laser weapons emit highly focused beams of light that deal fire damage. These beams can pass through glass and other transparent physical barriers, dealing damage to such barriers as they pass through. Barriers of energy or magical force block lasers. Invisible creatures don’t take damage from lasers, as the beams pass through them harmlessly. Fog, smoke, and other clouds provide both cover and concealment from laser attacks. Lasers can penetrate darkness, but they don’t provide any illumination.</p>\n<p>Laser weapons use various means to concentrate beams of light into deadly intensity. Some focus light through a faceted crystal, while others focus the beam through a chemical cloud or ionized gas.</p>","chat":"","unidentified":""},"source":"Core Rulebook","type":"","quantity":1,"bulk":"3","price":14300,"level":9,"attuned":false,"equipped":false,"identified":true,"activation":{"type":"none","cost":0,"condition":""},"duration":{"value":"","units":""},"target":{"value":"","type":""},"area":{"value":null,"units":"none","shape":"","effect":""},"range":{"value":120,"units":"ft","additional":"","per":""},"uses":{"value":0,"max":0,"per":""},"ability":"dex","actionType":"rwak","attackBonus":0,"chatFlavor":"","critical":{"parts":[],"effect":""},"damage":{"parts":[["3d8","fire"]]},"formula":"","save":{"type":"","dc":null,"descriptor":"negate"},"descriptors":[],"capacity":{"value":40,"max":40},"usage":{"value":4,"per":"round"},"weaponType":"heavy","special":"","properties":{"one":false,"two":true,"amm":false,"analog":false,"archaic":false,"automatic":false,"blast":false,"block":false,"boost":false,"bright":false,"disarm":false,"entangle":false,"explode":false,"injection":false,"line":false,"nonlethal":false,"operative":false,"penetrating":true,"powered":false,"qreload":false,"reach":false,"sniper":false,"stun":false,"thrown":false,"trip":false,"unwieldy":false,"radioactive":false,"subtle":false,"mind-affecting":false,"living":false},"proficient":true,"fusions":[],"abilityMods":{"parts":[]}},"flags":{},"img":"systems/sfrpg/icons/equipment/weapons/artillery-laser-aphelion.jpg"}
{"name":"Sonic rifle, banshee","permission":{"default":0,"EgDbkW0aYa1c3C2f":3},"type":"weapon","data":{"description":{"value":"<p>A sonic rifle fires a blast that is loud enough to deafen targets in addition to damaging them. Sonic rifles have snub-nosed barrels and typically incorporate U-shaped prongs to improve beam stability.</p>","chat":"","unidentified":""},"source":"SCR","type":"","quantity":1,"bulk":"2","price":80200,"level":14,"attuned":false,"equipped":false,"identified":true,"activation":{"type":"action","cost":1,"condition":""},"duration":{"value":"","units":""},"target":{"value":"","type":""},"area":{"value":null,"units":null,"shape":"","effect":""},"range":{"value":50,"units":"ft","additional":"","per":""},"uses":{"value":0,"max":0,"per":""},"ability":"dex","actionType":"rwak","attackBonus":0,"chatFlavor":"Deafen","critical":{"parts":[],"effect":""},"damage":{"parts":[["6d10","sonic"]]},"formula":"","save":{"type":"","dc":null,"descriptor":"negate"},"descriptors":[],"capacity":{"value":100,"max":100},"usage":{"value":5,"per":"5"},"modifiers":[],"weaponType":"longA","special":"","properties":{"one":false,"two":true,"amm":false,"analog":false,"archaic":false,"automatic":false,"blast":false,"block":false,"boost":false,"bright":false,"disarm":false,"entangle":false,"explode":false,"injection":false,"line":false,"living":false,"mind-affecting":false,"nonlethal":false,"operative":false,"penetrating":false,"powered":false,"qreload":false,"radioactive":false,"reach":false,"sniper":false,"stun":false,"subtle":false,"thrown":false,"trip":false,"unwieldy":false,"aeon":false,"antibiological":false,"aurora":false,"breach":false,"breakdown":false,"cluster":false,"conceal":false,"deconstruct":false,"deflect":false,"double":false,"drainCharge":false,"echo":false,"extinguish":false,"feint":false,"fiery":false,"firstArc":false,"flexibleLine":false,"force":false,"freeHands":false,"fueled":false,"grapple":false,"gravitation":false,"guided":false,"harrying":false,"holyWate