{"name":"Tactical X-gen Gun","permission":{"default":0,"EgDbkW0aYa1c3C2f":3},"type":"weapon","data":{"description":{"value":"<p>X-gen guns are named for their external generator&mdash;a power pack that drives the automatic feed of the weapon. They are common on fortified bases and in large armed encampments.</p>","chat":"","unidentified":""},"source":"SCR","type":"","quantity":1,"bulk":"2","price":4240,"level":6,"attuned":false,"equipped":false,"identified":true,"activation":{"type":"action","cost":1,"condition":""},"duration":{"value":"","units":""},"target":{"value":"","type":""},"area":{"value":null,"units":null,"shape":"","effect":""},"range":{"value":120,"units":"ft","additional":"","per":""},"uses":{"value":0,"max":0,"per":""},"ability":"dex","actionType":"rwak","attackBonus":0,"chatFlavor":"","critical":{"parts":[],"effect":""},"damage":{"parts":[["1d12","piercing"]]},"formula":"","save":{"type":"","dc":null,"descriptor":"negate"},"descriptors":[],"capacity":{"value":80,"max":80},"usage":{"value":2,"per":"2"},"modifiers":[],"weaponType":"heavy","special":"","properties":{"one":false,"two":true,"amm":false,"analog":true,"archaic":false,"automatic":true,"blast":false,"block":false,"boost":false,"bright":false,"disarm":false,"entangle":false,"explode":false,"injection":false,"line":false,"living":false,"mind-affecting":false,"nonlethal":false,"operative":false,"penetrating":false,"powered":false,"qreload":false,"radioactive":false,"reach":false,"sniper":false,"stun":false,"subtle":false,"thrown":false,"trip":false,"unwieldy":false,"aeon":false,"antibiological":false,"aurora":false,"breach":false,"breakdown":false,"cluster":false,"conceal":false,"deconstruct":false,"deflect":false,"double":false,"drainCharge":false,"echo":false,"extinguish":false,"feint":false,"fiery":false,"firstArc":false,"flexibleLine":false,"force":false,"freeHands":false,"fueled":false,"grapple":false,"gravitation":false,"guided":false,"harrying":false,"holyWater":false,"ignite":false,"indir