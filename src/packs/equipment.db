{"_id":"zgNvYv8ZZatEqFk3","name":"Ifrit-class Flamethrower","permission":{"default":0,"EgDbkW0aYa1c3C2f":3},"type":"weapon","data":{"description":{"value":"<p>These portable flamethrowers consist of a heavy rifle-like design with an oversized petrol tank integrated into the weapon&rsquo;s stock.</p>","chat":"","unidentified":""},"source":"SCR","type":"","quantity":1,"bulk":"2","price":780,"level":2,"attuned":false,"equipped":false,"identified":true,"activation":{"type":"action","cost":1,"condition":""},"duration":{"value":"","units":""},"target":{"value":"","type":""},"area":{"value":null,"units":"none","shape":"","effect":""},"range":{"value":15,"units":"ft","additional":"","per":""},"uses":{"value":0,"max":0,"per":""},"ability":"dex","actionType":"rwak","attackBonus":0,"chatFlavor":"Burn [[1d6]]","critical":{"parts":[],"effect":""},"damage":{"parts":[["1d6","fire"]]},"formula":"","save":{"type":"","dc":null,"descriptor":"negate"},"descriptors":[],"capacity":{"value":20,"max":20},"usage":{"value":4,"per":"4"},"modifiers":[],"weaponType":"heavy","special":"","properties":{"one":false,"two":true,"amm":false,"analog":true,"archaic":false,"automatic":false,"blast":true,"block":false,"boost":false,"bright":false,"disarm":false,"entangle":false,"explode":false,"injection":false,"line":false,"living":false,"mind-affecting":false,"nonlethal":false,"operative":false,"penetrating":false,"powered":false,"qreload":false,"radioactive":false,"reach":false,"sniper":false,"stun":false,"subtle":false,"thrown":false,"trip":false,"unwieldy":true,"aeon":false,"antibiological":false,"aurora":false,"breach":false,"breakdown":false,"cluster":false,"conceal":false,"deconstruct":false,"deflect":false,"double":false,"drainCharge":false,"echo":false,"extinguish":false,"feint":false,"fiery":false,"firstArc":false,"flexibleLine":false,"force":false,"freeHands":false,"fueled":false,"grapple":false,"gravitation":false,"guided":false,"harrying":false,"holyWater":false,"ignite":false,"indirect":fa