{"name":"Doshakari, zero-edge","permission":{"default":0,"fBSp8kiWWeNwQKW7":3},"type":"weapon","data":{"description":{"value":"<p>This punch dagger resembles a miniature doshko extending from the knuckles, with one to four diamond‑shaped blades. Doshakaris come in most of the same varieties as traditional steel doshkos, including the translucent molecular rift version.</p>","chat":"","unidentified":""},"source":"Near Space pg. 148 ","type":"","quantity":1,"bulk":"L","price":175000,"level":16,"attuned":false,"equipped":false,"identified":true,"activation":{"type":"action","cost":1,"condition":""},"duration":{"value":"","units":""},"target":{"value":"","type":""},"area":{"value":null,"units":null,"shape":"","effect":""},"range":{"value":null,"units":null,"additional":"","per":""},"uses":{"value":0,"max":0,"per":""},"ability":"str","actionType":"mwak","attackBonus":0,"chatFlavor":"","critical":{"parts":[],"effect":""},"damage":{"parts":[["6d6","piercing"]]},"formula":"Wound","save":{"type":"","dc":null,"descriptor":"negate"},"descriptors":[],"capacity":{"value":0,"max":0},"usage":{"value":0,"per":""},"weaponType":"advancedM","special":"","properties":{"one":true,"two":false,"amm":false,"analog":true,"archaic":false,"automatic":false,"blast":false,"block":false,"boost":false,"bright":false,"disarm":true,"entangle":false,"explode":false,"injection":false,"line":false,"living":false,"mind-affecting":false,"nonlethal":false,"operative":true,"penetrating":false,"powered":false,"qreload":false,"radioactive":false,"reach":false,"sniper":false,"stun":false,"subtle":false,"thrown":false,"trip":false,"unwieldy":false},"proficient":true,"fusions":[],"abilityMods":{"parts":[]}},"flags":{},"img":"icons/svg/mystery-man.svg","_id":"zphwb3wPAnuxCis1"}
