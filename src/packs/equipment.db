{"_id":"MiO2iepRGDYdycds","name":"Static Shock Caster","permission":{"default":0,"EgDbkW0aYa1c3C2f":3},"type":"weapon","data":{"description":{"value":"<p>Shock casters fire a blast of electrical energy that explodes when it impacts, creating an electrical storm that can damage and stun anyone within its radius.</p>","chat":"","unidentified":""},"source":"SCR","type":"","quantity":1,"bulk":"2","price":4620,"level":6,"attuned":false,"equipped":false,"identified":true,"activation":{"type":"action","cost":1,"condition":""},"duration":{"value":"","units":""},"target":{"value":"","type":""},"area":{"value":10,"units":"ft","shape":"shapable","effect":""},"range":{"value":40,"units":"ft","additional":"","per":""},"uses":{"value":0,"max":0,"per":""},"ability":"dex","actionType":"rwak","attackBonus":0,"chatFlavor":"","critical":{"parts":[],"effect":""},"damage":{"parts":[["1d12","electricity"]]},"formula":"","save":{"type":"reflex","dc":null,"descriptor":"half"},"descriptors":[],"capacity":{"value":40,"max":40},"usage":{"value":2,"per":"2"},"modifiers":[],"weaponType":"heavy","special":"","properties":{"one":false,"two":true,"amm":false,"analog":false,"archaic":false,"automatic":false,"blast":false,"block":false,"boost":false,"bright":false,"disarm":false,"entangle":false,"explode":true,"injection":false,"line":false,"living":false,"mind-affecting":false,"nonlethal":false,"operative":false,"penetrating":false,"powered":false,"qreload":false,"radioactive":false,"reach":false,"sniper":false,"stun":false,"subtle":false,"thrown":false,"trip":false,"unwieldy":true,"aeon":false,"antibiological":false,"aurora":false,"breach":false,"breakdown":false,"cluster":false,"conceal":false,"deconstruct":false,"deflect":false,"double":false,"drainCharge":false,"echo":false,"extinguish":false,"feint":false,"fiery":false,"firstArc":false,"flexibleLine":false,"force":false,"freeHands":false,"fueled":false,"grapple":false,"gravitation":false,"guided":false,"harrying":false,"holyWater":false,"ignit