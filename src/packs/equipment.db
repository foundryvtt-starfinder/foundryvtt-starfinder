{"name":"Infernian, ifrit-class","permission":{"default":0,"fBSp8kiWWeNwQKW7":3},"type":"weapon","data":{"description":{"value":"<p>Produced in prison-factories on Embroi, this short pistol uses clips of magnesium flares to deliver shots of scorching fire. The fire from this weapon seems to dance briefly over the target, lunging out toward other creatures.</p>","chat":"","unidentified":""},"source":"Near Space pg. 148","type":"","quantity":1,"bulk":"L","price":1400,"level":3,"attuned":false,"equipped":false,"identified":true,"activation":{"type":"action","cost":1,"condition":""},"duration":{"value":"","units":""},"target":{"value":"","type":""},"area":{"value":null,"units":null,"shape":"","effect":""},"range":{"value":50,"units":"ft","additional":"","per":""},"uses":{"value":0,"max":0,"per":""},"ability":"dex","actionType":"rwak","attackBonus":0,"chatFlavor":"","critical":{"parts":[],"effect":""},"damage":{"parts":[["1d6","fire"]]},"formula":"","save":{"type":"","dc":null,"descriptor":"negate"},"descriptors":[],"capacity":{"value":4,"max":4},"usage":{"value":1,"per":"flare"},"weaponType":"smallA","special":"","properties":{"one":true,"two":false,"amm":true,"analog":false,"archaic":false,"automatic":false,"blast":false,"block":false,"boost":false,"bright":false,"disarm":false,"entangle":false,"explode":false,"injection":false,"line":false,"living":false,"mind-affecting":false,"nonlethal":false,"operative":false,"penetrating":false,"powered":false,"qreload":false,"radioactive":false,"reach":false,"sniper":false,"stun":false,"subtle":false,"thrown":false,"trip":false,"unwieldy":false},"proficient":true,"fusions":[],"abilityMods":{"parts":[]}},"flags":{},"img":"icons/svg/mystery-man.svg","_id":"fYNA9t5lVNbLtAnA"}
{"_id":"fc7L7RdJpnmLmmYH","name":"Plasma pistol, blue star","permission":{"default":0},"type":"weapon","data":{"description":{"value":"<p>Plasma pistols fire a line of ionized plasma out to a relatively short range. The plasma can continue to burn after contact.</p>","chat":"","unidentified":""},"source":"Core Rulebook, P. 173","type":"","quantity":1,"bulk":"L","price":565000,"level":19,"attuned":false,"equipped":false,"identified":true,"activation":{"type":"none","cost":0,"condition":""},"duration":{"value":"","units":""},"target":{"value":"","type":""},"area":{"value":40,"units":"ft","shape":"line","effect":""},"range":{"value":null,"units":"none","additional":"","per":""},"uses":{"value":0,"max":0,"per":""},"ability":"dex","actionType":"rwak","attackBonus":0,"chatFlavor":"","critical":{"parts":[],"effect":""},"damage":{"parts":[["5d8","electricity"],["","fire"]]},"formula":"","save":{"type":"","dc":null,"descriptor":"negate"},"descriptors":[],"capacity":{"value":100,"max":100},"usage":{"value":20,"per":"round"},"weaponType":"smallA","special":"","properties":{"one":true,"two":false,"amm":false,"analog":false,"archaic":false,"automatic":false,"blast":false,"block":false,"boost":false,"bright":false,"disarm":false,"entangle":false,"explode":false,"injection":false,"line":true,"nonlethal":false,"operative":false,"penetrating":false,"powered":false,"qreload":false,"reach":false,"sniper":false,"stun":false,"thrown":false,"trip":false,"unwieldy":true,"aeon":false,"antibiological":false,"aurora":false,"breach":false,"breakdown":false,"cluster":false,"conceal":false,"deconstruct":false,"deflect":false,"double":false,"drainCharge":false,"echo":false,"extinguish":false,"feint":false,"fiery":false,"firstArc":false,"flexibleLine":false,"force":false,"freeHands":false,"fueled":false,"grapple":false,"gravitation":false,"guided":false,"harrying":false,"holyWater":false,"ignite":false,"indirect":false,"integrated":false,"living":false,"lockdown":false,"mind-affecting":false,"min