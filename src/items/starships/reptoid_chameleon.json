    {
      "_id": "3gSuZuChMDgVIuqn",
      "name": "Holographic Mantle",
      "type": "starshipSecuritySystem",
      "img": "icons/svg/mystery-man.svg",
      "system": {
        "type": "",
        "abilityMods": {
          "parts": []
