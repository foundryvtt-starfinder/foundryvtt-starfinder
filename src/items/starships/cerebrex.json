    {
      "_id": "FFoLtCT0gFy7NKl2",
      "name": "Intelligence Drain",
      "type": "starshipOtherSystem",
      "effects": [],
      "flags": {},
      "folder": null,
      "img": "icons/svg/mystery-man.svg",
      "permission": {
        "cX0Tohf6UKZbiy0U": 3,
