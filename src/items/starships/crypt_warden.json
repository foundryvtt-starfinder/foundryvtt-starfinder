    {
      "_id": "ki6uMgL36dmRTDqA",
      "name": "Common crew quarters",
      "type": "starshipCrewQuarter",
      "_stats": {
        "coreVersion": 12,
        "systemId": "sfrpg",
        "systemVersion": "0.26.0"
      },
      "img": "icons/svg/mystery-man.svg",
      "system": {
        "type": "",
