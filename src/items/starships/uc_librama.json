    {
      "_id": "5iRab0YiiMrMTUjt",
      "name": "Anti-hacking systems",
      "type": "starshipSecuritySystem",
      "img": "icons/svg/mystery-man.svg",
      "system": {
        "type": "",
        "abilityMods": {
          "parts": []
