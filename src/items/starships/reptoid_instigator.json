    {
      "_id": "NRHnex2iDFRr4wmR",
      "name": "Reconfiguration System",
      "type": "starshipSecuritySystem",
      "_stats": {
        "coreVersion": 12,
        "systemId": "sfrpg",
        "systemVersion": "0.26.1"
      },
      "img": "icons/svg/mystery-man.svg",
      "system": {
        "type": "",
        "abilityMods": {
          "parts": []
