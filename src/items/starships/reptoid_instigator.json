    {
      "_id": "tZqGRRjCMjYRR9Hy",
      "name": "Cultural preparation facility",
      "type": "starshipExpansionBay",
      "_stats": {
        "coreVersion": 12,
        "systemId": "sfrpg",
        "systemVersion": "0.26.1"
      },
      "img": "icons/svg/mystery-man.svg",
      "system": {
        "type": "",
        "abilityMods": {
