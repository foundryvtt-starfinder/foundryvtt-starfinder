    {
      "_id": "D9GUIdbwyjB4uXgC",
      "name": "Powersap",
      "type": "starshipOtherSystem",
      "effects": [],
      "flags": {},
      "img": "icons/svg/mystery-man.svg",
      "sort": 1800000,
      "system": {
        "type": "",
        "abilityMods": {
          "parts": []
