        "cost": 8,
        "costMultipliedBySize": false,
        "description": {
          "chat": "",
          "gmnotes": "",
          "short": "",
          "unidentified": "",
          "value": "<p>A medical bay functions as a medical lab (see page 220)</p>"
        },
        "isPowered": true,
        "modifiers": [],
        "pcu": 4,
        "slug": "",
        "source": "CRB pg. 300"
      }
    }
