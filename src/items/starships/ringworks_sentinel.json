    {
      "_id": "sNxoKLhVwIqMopOi",
      "name": "Good crew quarters",
      "type": "starshipCrewQuarter",
      "_stats": {
        "coreVersion": 12,
        "systemId": "sfrpg",
        "systemVersion": "0.26.1"
