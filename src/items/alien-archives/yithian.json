    {
      "_id": "FuNX9vk1Lq3CUUxw",
      "name": "Detect Thoughts (Range 100 Ft. DC 16) (At Will)",
      "type": "spell",
      "_stats": {
        "coreVersion": 12,
        "systemId": "sfrpg",
        "systemVersion": "0.26.1"
