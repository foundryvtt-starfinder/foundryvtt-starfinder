      "name": "Nondetection (At Will)",
      "type": "spell",
      "img": "systems/sfrpg/icons/spells/nondetection.webp",
      "system": {
        "type": "",
        "ability": "",
        "abilityMods": {
          "parts": []
        },
        "actionType": "save",
        "activation": {
          "type": "action",
