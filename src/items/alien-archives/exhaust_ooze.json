      },
      "img": "icons/svg/mystery-man.svg",
      "system": {
        "type": "",
        "ability": null,
        "abilityMods": {
          "parts": []
        },
        "actionTarget": "",
        "actionType": "",
        "activation": {
          "type": "action",
