    "details": {
      "type": "Monstrous Humanoid",
      "alignment": "LN",
      "aura": "",
      "biography": {
        "age": 0,
        "dateOfBirth": "",
        "deity": "",
        "fullBodyImage": "systems/sfrpg/images/mystery-body.webp",
        "genderPronouns": "",
