    {
      "name": "Spring Attack (Combat)",
      "type": "feat",
      "flags": {},
      "img": "systems/sfrpg/icons/feats/spring_attack.webp",
      "system": {
        "type": "",
        "ability": null,
        "actionType": "",
        "activation": {
          "type": "",
