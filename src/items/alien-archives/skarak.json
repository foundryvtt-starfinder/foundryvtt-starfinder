    {
      "_id": "q2W6m0XAW59TxQx1",
      "name": "Snare (Su)",
      "type": "feat",
      "_stats": {
        "coreVersion": 12,
        "systemId": "sfrpg",
        "systemVersion": "0.26.1"
      },
      "img": "icons/svg/mystery-man.svg",
      "system": {
        "type": "",
        "ability": "",
        "abilityMods": {
          "parts": []
        },
        "actionTarget": "",
        "actionType": "save",
        "activation": {
          "type": "",
