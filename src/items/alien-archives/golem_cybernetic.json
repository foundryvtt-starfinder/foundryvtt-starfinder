      "_id": "MhUqdlVDrluGELB2",
      "name": "Magic Immunity (Ex)",
      "type": "feat",
      "img": "systems/sfrpg/icons/default/achievement.svg",
      "system": {
        "ability": "",
        "actionTarget": "",
        "actionType": "",
        "activation": {
