    "details": {
      "type": " Construct (technological)",
      "alignment": "N",
      "aura": "",
      "biography": {
        "age": 0,
        "dateOfBirth": "",
        "deity": "",
        "fullBodyImage": "systems/sfrpg/images/mystery-body.webp",
        "genderPronouns": "",
