    {
      "_id": "cZ3oCJVtSrtuf5MW",
      "name": "Symbiote Adaption",
      "type": "feat",
      "img": "systems/sfrpg/icons/default/achievement.svg",
      "system": {
        "ability": "",
        "actionTarget": "",
        "actionType": "",
