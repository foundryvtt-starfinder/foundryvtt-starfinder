    {
      "_id": "AjmbrIGxZvDKBPGH",
      "name": "Envoy Improvisation",
      "type": "feat",
      "img": "systems/sfrpg/icons/classes/envoy_improvisation.webp",
      "system": {
        "type": "",
        "ability": null,
        "abilityMods": {
          "parts": []
        },
        "actionType": "",
        "activation": {
          "type": "",
          "condition": "",
