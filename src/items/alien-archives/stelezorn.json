      "_id": "NNRQn1n3EnHzOtKP",
      "name": "Change Shape (humanoid)",
      "type": "feat",
      "_stats": {
        "coreVersion": 12,
        "systemId": "sfrpg",
        "systemVersion": "0.26.0"
      },
      "img": "icons/svg/mystery-man.svg",
      "system": {
        "type": "",
