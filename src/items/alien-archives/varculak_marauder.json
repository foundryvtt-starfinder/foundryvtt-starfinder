    {
      "_id": "0hCdlGNZNip0Dnu0",
      "name": "Blitz",
      "type": "feat",
      "img": "systems/sfrpg/icons/classes/blitz.webp",
      "system": {
        "ability": "",
        "actionTarget": "",
        "actionType": "",
