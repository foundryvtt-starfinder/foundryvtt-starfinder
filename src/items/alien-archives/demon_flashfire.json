    {
      "_id": "0V2ARq3sJinNgRmW",
      "name": "Mental Block (DC 22)",
      "type": "spell",
      "img": "systems/sfrpg/icons/spells/mental_block.PNG",
      "data": {
        "description": {
          "chat": "",
          "gmnotes": "",
          "short": "",
