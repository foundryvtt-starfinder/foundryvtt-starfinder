      },
      "img": "icons/svg/item-bag.svg",
      "system": {
        "type": "",
        "ability": "",
        "abilityMods": {
          "parts": []
        },
        "actionTarget": "",
        "actionType": "save",
        "activation": {
          "type": "full",
          "condition": "",
          "cost": 1
        },
        "area": {
          "effect": "",
          "shape": "",
          "units": "none",
          "value": null
        },
        "attackBonus": 0,
