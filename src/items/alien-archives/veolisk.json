    "details": {
      "type": "Magical Beast",
      "alignment": "CE",
      "biography": {
        "age": "0",
        "dateOfBirth": "",
        "deity": "",
        "fullBodyImage": "systems/sfrpg/images/mystery-body.webp",
        "genderPronouns": "",
