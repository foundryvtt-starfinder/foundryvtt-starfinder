      "name": "Microbot Assault",
      "type": "spell",
      "flags": {},
      "img": "systems/sfrpg/icons/spells/microbot_assault.webp",
      "sort": 1200001,
      "system": {
        "type": "",
        "ability": null,
        "abilityMods": {
          "parts": []
