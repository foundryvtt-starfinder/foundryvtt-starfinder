    {
      "_id": "UwgAt6bweUfyfL5W",
      "name": "Battery, Standard",
      "type": "goods",
      "flags": {},
      "img": "systems/sfrpg/icons/equipment/weapons/battery-standard.webp",
      "sort": 1000001,
      "system": {
