    {
      "name": "Battery, Standard",
      "type": "goods",
      "hasAttack": false,
      "img": "systems/sfrpg/icons/equipment/weapons/battery-standard.webp",
      "isCharged": true,
      "isStack": true,
      "labels": {},
      "system": {
        "type": "",
        "attuned": false,
        "bulk": "-",
        "damage": {
