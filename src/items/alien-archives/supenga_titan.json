    "details": {
      "type": "Plant",
      "alignment": "N",
      "aura": "Gnawing brood - 10 ft., Ref DC 21",
      "biography": {
        "age": 0,
        "dateOfBirth": "",
        "deity": "",
        "fullBodyImage": "systems/sfrpg/images/mystery-body.webp",
        "genderPronouns": "",
