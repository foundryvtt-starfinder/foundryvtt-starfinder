    "details": {
      "type": "Monstrous Humanoid (aquatic)",
      "alignment": "NG",
      "aura": "",
      "biography": {
        "age": 0,
        "dateOfBirth": "",
        "deity": "",
        "fullBodyImage": "systems/sfrpg/images/mystery-body.webp",
        "genderPronouns": "",
