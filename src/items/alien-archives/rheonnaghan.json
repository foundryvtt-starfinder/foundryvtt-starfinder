    {
      "_id": "0LDUEQc6JoKbPQ5d",
      "name": "Stellar Alignment",
      "type": "feat",
      "data": {
        "type": "",
        "ability": null,
        "abilityMods": {
          "parts": []
        },
        "actionType": "",
        "activation": {
          "type": "",
