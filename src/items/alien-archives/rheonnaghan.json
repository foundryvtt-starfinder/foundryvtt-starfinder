    {
      "_id": "WciXG7lir7W9dJER",
      "name": "Penumbral Reflection (Su)",
      "type": "feat",
      "data": {
        "type": "",
        "ability": "",
        "abilityMods": {
          "parts": []
        },
        "actionType": "rwak",
        "activation": {
          "type": "reaction",
