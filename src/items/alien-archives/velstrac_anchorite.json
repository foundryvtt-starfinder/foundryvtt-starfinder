    "details": {
      "type": "Outsider (evil, extraplanar, velstrac, lawful)",
      "alignment": "LE",
      "aura": "Unnerving gaze - 30 ft [DC 13]",
      "biography": {
        "age": 0,
        "dateOfBirth": "",
