    {
      "name": "Fear (3rd Level, DC 18)",
      "type": "spell",
      "data": {
        "type": "",
        "ability": "",
        "abilityMods": {
          "parts": []
        },
        "actionType": "save",
        "activation": {
          "type": "action",
          "condition": "",
          "cost": 1
