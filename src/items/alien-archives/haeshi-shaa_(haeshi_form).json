    {
      "name": "Mind Thrust (4th Level, DC 19)",
      "type": "spell",
      "data": {
        "type": "",
        "ability": "",
        "abilityMods": {
          "parts": []
        },
        "actionType": "save",
        "activation": {
          "type": "action",
